{
  "name": "@notifica/react",
  "version": "0.2.0",
  "description": "Componente React embeddable de notificações in-app para Notifica",
  "license": "MIT",
  "author": "Notifica <dev@usenotifica.com.br>",
  "homepage": "https://docs.usenotifica.com.br/sdks/react",
  "repository": {
    "type": "git",
    "url": "https://github.com/notifica-dev/notifica",
    "directory": "src/sdks/react"
  },
  "keywords": [
    "notifica",
    "notifications",
    "inbox",
    "bell",
    "react",
    "in-app",
    "brazil"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@testing-library/react": "^14.2.0",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
